            </column>
            <column name="DESCRIPTION" type="VARCHAR(255)"/>
            <column name="CATEGORY" type="VARCHAR(255)"/>
            <column name="ASSET_CODE" type="VARCHAR(255)"/>
            <column name="IMAGE" type="VARCHAR(1024)"/>
            <column name="LEVEL_ID" type="UUID"/>
        </createTable>
    </changeSet>
